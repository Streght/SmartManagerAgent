<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartmanageragent.application">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AboutActivity" />
        <activity android:name=".TestComActivity" />
        <activity
            android:name=".CalendarActivity"
            android:label="@string/title_calendar_activity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MeetingsActivity"
            android:label="@string/title_meetings_activity" />
        <activity
            android:name=".MeetingAddActivity"
            android:label="@string/title_meeting_add_activity" />
        <activity android:name=".WeeklyFreeTimeActivity"
            android:label="@string/title_meeting_add_activity"/>
        <activity android:name=".TestListIdActivity" />

        <service android:name="com.smartmanageragent.exteriorCommunication.ServiceCommunication" />
        <service
            android:name="com.smartmanageragent.smartagent.AgentService"
            android:exported="false" />
    </application>

</manifest>